<VirtualHost *:80>
	ServerName <%= @host_name %>
	ProxyPass / http://localhost:<%=node["artifactory"]["port"] %>/
	ProxyPassReverse / http://localhost:<%=node["artifactory"]["port"] %>/
	ProxyRequests Off
	ProxyPreserveHost on
	
	<Proxy http://localhost:<%=node["artifactory"]["port"] %>/*>
    	Order deny,allow
    	Allow from all
	</Proxy>

</VirtualHost>
